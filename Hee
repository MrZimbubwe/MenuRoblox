    clickSound:Play()  

    if canCollide then
        textButton.Text = "‡∏ó‡∏∞‡∏•‡∏∏‡∏Å‡∏≥‡πÅ‡∏û‡∏á: ‡∏õ‡∏¥‡∏î"
        textButton.BorderColor3 = Color3.fromRGB(200, 200, 200)
